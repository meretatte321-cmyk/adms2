{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "TURSO_DB_URL": "libsql://adms-adms-server.aws-ap-south-1.turso.io",
    "TURSO_DB_TOKEN": "@turso_token",
    "MINUTES_FOR_PRESENT": "360"
  }
}
